{
  "name": "@stoquiz/backend",
  "version": "1.0.0",
  "description": "StoQuiz backend API",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "prebuild": "pnpm exec prisma generate --schema=./prisma/schema.prod.prisma",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "db:generate": "prisma generate --schema=./prisma/schema.prod.prisma",
    "generate:prod": "prisma generate --schema=./prisma/schema.prod.prisma",
    "db:migrate": "prisma migrate dev --schema=./prisma/schema.prod.prisma",
    "db:push": "prisma db push --schema=./prisma/schema.prod.prisma",
    "db:studio": "prisma studio --schema=./prisma/schema.prod.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@stoquiz/db": "workspace:*",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prisma": "^5.6.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  }
}